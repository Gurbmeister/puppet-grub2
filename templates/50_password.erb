#!/bin/sh
#
# WARNING: This file maintained by Puppet.
# Editing is no use unless you avoid running the Puppet agent
#
# Set username and PBKDF2 password for protect GRUB configuration
# during the boot process.
#
exec tail -n +3 $0
<% if @password -%>
set superusers="<%= @password_username %>"
password_pbkdf2 <%= @password_username %> <%= @password_pbkdf2_hash %>
<% end -%>
